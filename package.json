{
  "name": "youibot-tms-web",
  "version": "4.0.0",
  "description": "TMS平台",
  "main": "index.js",
  "scripts": {
    "start": "npm i -g pnpm && pnpm i & pnpm prepare & pnpm dev",
    "dev": "node ./plugins/devCopyFonts.js && vite --open",
    "build": "vite build && node ./plugins/copyFonts.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "commit": " git add . && git cz",
    "preinstall": "npx only-allow pnpm -y",
    "prepare": "husky install",
    "lint": "pnpm run lint:es && pnpm run lint:style",
    "lint:es": "npx eslint --cache --ext .js,.tsx,.ts,.md,.json,.vue src/",
    "lint:style": "npx stylelint --cache src/**/*.{css,scss,less,vue}"
  },
  "repository": {
    "type": "git",
    "url": "git@gitee.com:youibot_system/ADSWeb.git"
  },
  "author": "zhangjunrong@youibot.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.18.12",
    "@types/postcss-load-config": "^2.0.1",
    "@types/requestidlecallback": "^0.3.4",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-legacy": "^1.7.1",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/cli-plugin-unit-jest": "^4.5.17",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.0.0",
    "@vue/test-utils": "^2.0.0-rc.17",
    "@vueuse/core": "^7.7.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-import": "^1.13.3",
    "babel-preset-env": "^1.7.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-conventional-changelog-zh": "^0.0.2",
    "enquire-js": "^0.2.1",
    "esbuild": "~0.12.29",
    "esbuild-loader": "^2.18.0",
    "escape-html": "^1.0.3",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-jsdoc": "^37.9.7",
    "eslint-plugin-markdown": "^2.0.0",
    "eslint-plugin-no-explicit-type-exports": "^0.12.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^25.5.4",
    "jest-canvas-mock": "^2.3.1",
    "less": "^4.1.2",
    "lint-staged": "^12.3.7",
    "postcss-html": "^1.3.0",
    "prettier": "^2.6.1",
    "sass": "^1.49.9",
    "stylelint": "^14.6.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-less": "^1.0.5",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "stylelint-selector-no-empty": "^1.0.8",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.10",
    "unplugin-auto-import": "^0.8.7",
    "unplugin-vue-components": "^0.19.6",
    "vite": "2.9.10",
    "vite-plugin-style-import": "^0.9.2",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.0.15"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,tsx,ts,less,md,json,vue}": [
      "npm run lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-zh"
    }
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "3.2.0",
    "axios": "^0.26.1",
    "postcss": "8",
    "vue": "^3.2.31",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2",
    "youibot-plus": "1.2.4-7"
  }
}
